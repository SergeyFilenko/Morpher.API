<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net40</TargetFrameworks>
    <PackageId>Morpher.WebService.V3.Client</PackageId>
    <RootNamespace>Morpher.WebService.V3</RootNamespace>
    <Authors>Morpher.ru</Authors>
    <Title>Russian and Ukrainian declension web service client.</Title>
    <Copyright>Copyright 2017-2023 morpher.ru</Copyright>
    <PackageTags>declension pluralization russian ukrainian grammar morphology</PackageTags>
    <VersionSuffix>$(VersionSuffix)</VersionSuffix>
    <RepositoryUrl>https://github.com/morpher-ru/Morpher.WebService.V3.Client</RepositoryUrl>
    <PackageProjectUrl>https://morpher.ru/ws3</PackageProjectUrl>
    <PackageReleaseNotes>UnknownResponseException.</PackageReleaseNotes>

    <!-- These are set in appveyor.yml -->
    <PackageVersion>1.0.0</PackageVersion>
    <AssemblyVersion>1.0.0</AssemblyVersion>
    <FileVersion>1.0.0</FileVersion>
    <InformationalVersion>1.0.0</InformationalVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.*" />
  </ItemGroup>

</Project>
